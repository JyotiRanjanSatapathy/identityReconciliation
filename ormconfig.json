{
    "type": "postgres",
    "synchronize": true,
    "logging": false,
    "entities": [
        "dist/entity/**/*.js"
    ],
    "cli": {
        "entitiesDir": "dist/entity",
        "migrationsDir": "dist/migration",
        "subscribersDir": "dist/subscriber"
    },
    "migrations": [
        "dist/migration/**/*.js"
    ],
    "subscribers": [
        "src/subscriber/**/*.ts"
    ]
}